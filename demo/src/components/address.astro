---
import { Block, Seperator, GoogleMap } from "basecamp/components";
---

<Block.Wrapper id="address" variant="alternative">
    <Block.Seperator slot="seperator">
        <Seperator loading="lazy" />
    </Block.Seperator>
    <Block.Header slot="header" className="text-primary">
        Find Us
    </Block.Header>
    <Block.SubHeader slot="subHeader">
        Scout HQ, 1st, Staines Ln, Chertsey KT16 8PS.
    </Block.SubHeader>
    <Block.Body slot="body" size="md">
        <GoogleMap
            mapUrl="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2489.2856529569726!2d-0.509307878563761!3d51.39780582950432!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x48767792a09dbd7b%3A0x6fd3b0bba98c6c55!2s1st%20Chertsey%20Scout%20Group!5e0!3m2!1sen!2suk!4v1731404487795!5m2!1sen!2suk"
            mapTitle="Google Maps for 1st Chertsey Scout Group"
        />
    </Block.Body>
</Block.Wrapper>
